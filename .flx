[permission]
public_html 755

[setup]
rm -rf static .cabal
 
[deploy]
mkdir static
/usr/local/cabal/bin/cabal update
/usr/local/cabal/bin/cabal clean
/usr/local/cabal/bin/cabal install --one-shot
/bin/cp -v .cabal/bin/fluxflex-haskellinfo public_html/dispatch.fcgi
/bin/chmod -v 705 public_html/dispatch.fcgi
/bin/chown haskell-info:haskell-info public_html
ls -lha public_html
