[permission]
public_html 755

[setup]
rm -rf static .cabal
mkdir static

[deploy]
/usr/local/cabal/bin/cabal update
/usr/local/cabal/bin/cabal clean
/usr/local/cabal/bin/cabal install --disable-split-objs --enable-executable-stripping
/bin/cp .cabal/bin/fluxflex-haskellinfo public_html/fluxflex-haskellinfo.fcgi
ls -lha .cabal/bin/
ls -lha public_html/
/bin/chmod 705 public_html/fluxflex-haskellinfo.fcgi

